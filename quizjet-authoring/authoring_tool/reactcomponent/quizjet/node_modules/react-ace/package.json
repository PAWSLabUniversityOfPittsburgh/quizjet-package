{
  "name": "react-ace",
  "version": "2.8.0",
  "description": "A react component for Ace Editor",
  "main": "lib/ace.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "echo \"Temporarily remove\"",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/ace.jsx dist/react-ace.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/ace.jsx dist/react-ace.min.js --config webpack.config.production.js",
    "build:example": "cd example && npm install",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "check": "npm run lint",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "James Hrisho"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "jest": "^0.1.37",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2"
  },
  "keywords": [
    "ace",
    "ace editor",
    "react-component",
    "react",
    "brace"
  ],
  "dependencies": {
    "brace": "^0.5.1"
  },
